# GitHub API Token
# Get this from: https://github.com/settings/tokens
GITHUB_TOKEN=your_github_token_here

# Trello API Keys
# Get these from: https://trello.com/app-key
TRELLO_API_KEY=your_trello_api_key
TRELLO_TOKEN=your_trello_token
TRELLO_BOARD_ID=your_trello_board_id

# Optional: GitHub repository to analyze by default
DEFAULT_REPO=owner/repository_name

# AI API Keys (Optional - for enhanced code analysis)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here 